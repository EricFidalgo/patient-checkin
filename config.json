{
  "safety_stop": {
    "description": "Stops the user immediately if BMI is below FDA guidelines.",
    "enabled": true,
    "min_bmi": 27.0,
    "modal_id": "safety-stop-modal"
  },
  "ui_settings": {
    "loading_animation": {
      "steps": [
        { "percent": 30, "time_ms": 100 },
        { "percent": 60, "time_ms": 1500 },
        { "percent": 100, "time_ms": 3500 }
      ],
      "text_updates": [
        { "text": "Scanning 2026 Commercial Payer Rules...", "time_ms": 800 },
        { "text": "Identifying Pharmacy Partner Availability...", "time_ms": 1800 },
        { "text": "Calculating Copay & Savings Eligibility...", "time_ms": 2800 }
      ]
    }
  },
  "coverage_engine_config": {
    "Aetna": {
      "id": "aetna_2026",
      "default_status": "yellow",
      "member_id_validation": {
        "regex": "^(W\\d{9}|\\d{10}|\\d{12})$",
        "min_length": 10,
        "max_length": 12,
        "help_text": "Typically starts with 'W' followed by 9 digits, or is 10-12 numeric digits."
      },
      "rules": [
        {
          "if": { "planSource": "marketplace" },
          "then": { "status": "red", "reason": "Aetna is exiting the ACA Marketplace entirely effective Jan 1, 2026. Coverage terminates." }
        },
        {
          "if": { "medication": ["Compounded"] },
          "then": { "status": "red", "reason": "Exclusion: Non-FDA approved compounded agents are not covered." }
        },
        {
          "if": {
            "medication": ["Ozempic", "Mounjaro", "Trulicity", "Rybelsus", "Victoza"],
            "missing_comorbidities": ["diabetes"]
          },
          "then": { "status": "red", "reason": "Off-label use excluded. Diagnosis must be Type 2 Diabetes." }
        },
        {
          "if": {
            "medication": ["Zepbound"],
            "missing_history": ["Wegovy", "Saxenda"]
          },
          "then": { "status": "yellow", "reason": "Step Therapy: 'Wegovy First' protocol applies. Zepbound requires failure of Wegovy." }
        },
        {
          "if": {
            "medication": ["Wegovy", "Zepbound", "Saxenda"],
            "bmi_lt": 27
          },
          "then": { "status": "red", "reason": "BMI < 27 does not meet FDA or Aetna CPB 0039 criteria." }
        },
        {
          "if": {
            "medication": ["Wegovy", "Zepbound"],
            "bmi_range": [27, 30],
            "missing_comorbidities": ["hypertension", "high_cholesterol", "diabetes", "osa", "established_cvd"]
          },
          "then": { "status": "red", "reason": "BMI 27-30 requires specific comorbidities (HTN, CVD, OSA). PCOS/Fatty Liver alone are insufficient." }
        },
        {
          "if": {
            "medication": ["Wegovy", "Zepbound"],
            "missing_history": ["lifestyle_program"]
          },
          "then": { "status": "yellow", "reason": "Pre-authorization requires 6-month comprehensive weight management program history." }
        },
        {
          "if": {
             "medication": ["Wegovy"],
             "has_comorbidity": "established_cvd",
             "bmi_ge": 27
          },
          "then": { "status": "green", "reason": "Approved Indication: Risk reduction of MACE in patients with established CVD." }
        }
      ],
      "pricing": {
        "green_copay": 25,
        "lifestyle_program_discount": true
      }
    },
    "Cigna": {
      "id": "cigna_2026",
      "default_status": "yellow",
      "member_id_validation": {
        "regex": "^(U?[0-9]{8,11})$",
        "min_length": 9,
        "max_length": 11,
        "help_text": "Typically 9-11 digits. Legacy cards may start with 'U'."
      },
      "rules": [
        {
          "if": { "medication": ["Compounded"] },
          "then": { "status": "red", "reason": "Plan Exclusion: Compounded bio-similars are not covered." }
        },
        {
          "if": {
             "medication": ["Ozempic", "Mounjaro", "Trulicity", "Victoza", "Rybelsus"],
             "missing_comorbidities": ["diabetes"]
          },
          "then": { "status": "red", "reason": "Strict FDA Label enforcement. T2D diagnosis required." }
        },
        {
          "if": {
            "medication": ["Wegovy", "Zepbound"],
            "bmi_lt": 27
          },
          "then": { "status": "red", "reason": "Clinical criteria not met (BMI < 27)." }
        },
        {
          "if": {
             "medication": ["Wegovy", "Zepbound"],
             "bmi_range": [27, 30],
             "missing_comorbidities": ["hypertension", "diabetes", "high_cholesterol", "osa", "established_cvd"]
          },
          "then": { "status": "red", "reason": "Qualifying condition required for BMI 27-30 range." }
        },
        {
           "if": {
             "planSource": "marketplace",
             "state_not_in": ["CA", "MA", "NY", "VA"]
           },
           "then": { "status": "red", "reason": "2026 ACA Exchange plans in this state exclude weight loss medications." }
        },
        {
           "if": {
             "planSource": "govt",
             "medication": ["Wegovy", "Zepbound"],
             "missing_comorbidities": ["established_cvd"]
           },
           "then": { "status": "red", "reason": "Medicare statutory exclusion applies (unless Established CVD present)." }
        }
      ],
      "pricing": {
        "green_copay": 25,
        "lifestyle_program_discount": false
      }
    },
    "BCBS": {
      "id": "bcbs_2026",
      "default_status": "yellow",
      "member_id_validation": {
        "regex": "^(R\\d{8}|[A-Z0-9]{3}\\d{9})$",
        "min_length": 9,
        "max_length": 12,
        "help_text": "FEP starts with 'R' (9 chars). Commercial is 3 chars + 9 digits."
      },
      "rules": [
        {
          "if": { "medication": ["Compounded"] },
          "then": { "status": "red", "reason": "Non-FDA approved bulk powders are not covered benefits." }
        },
        {
          "if": {
             "state": "NJ",
             "medication": ["Wegovy", "Zepbound"],
             "bmi_lt": 35
          },
          "then": { "status": "red", "reason": "Horizon BCBS (NJ) 2026 Policy: Requires BMI 35+." }
        },
        {
          "if": {
             "state_in": ["MI", "MA"],
             "planSource": "employer"
          },
          "then": { "status": "red", "reason": "Default Exclusion in MI/MA: Requires specific Employer Rider." }
        },
        {
          "if": {
             "planSource": "govt",
             "medication": ["Wegovy", "Zepbound"]
          },
          "then": { "status": "green", "reason": "FEP Standard/Basic Option: Tier 2 Preferred Status." }
        },
        {
           "if": {
             "medication": ["Wegovy", "Zepbound"],
             "missing_history": ["lifestyle_program"]
           },
           "then": { "status": "yellow", "reason": "Requires proof of 6-month lifestyle program participation." }
        }
      ],
      "pricing": {
        "green_copay": 40,
        "lifestyle_program_discount": false
      }
    },
  "UHC": {
    "id": "uhc_2026",
    "default_status": "yellow",
    "member_id_validation": {
      "regex": "^([A-Z]{3,4})?\\d{9,12}$",
      "min_length": 9,
      "max_length": 16,
      "help_text": "9-12 digits, optional 3-4 letter prefix. Do not include suffixes (e.g. -01)."
    },
    "rules": [
      {
        "if": { "medication": ["Compounded"] },
        "then": { "status": "red", "reason": "Policy Exclusion: Compounded GLP-1s are considered unproven/experimental." }
      },
      {
        "if": {
          "medication": ["Ozempic", "Mounjaro", "Trulicity", "Victoza"],
          "missing_comorbidities": ["diabetes"]
        },
        "then": { "status": "red", "reason": "Diagnosis Mismatch: T2D diagnosis required for this brand." }
      },
      {
        "if": {
          "medication": ["Ozempic", "Mounjaro", "Trulicity"],
          "has_comorbidity": "diabetes"
        },
        "then": { "status": "green", "reason": "Standard Tier 2/3 coverage for Diabetes Management." }
      },
      {
        "if": { "planSource": "govt", "state_in": ["NC", "CA"] },
        "then": { "status": "red", "reason": "Medicaid coverage for weight loss ends in NC (Oct 2025) and CA (Jan 2026)." }
      },
      {
        "if": {
          "planSource": "marketplace",
          "state_not_in": ["CA", "MA", "NY"]
        },
        "then": { "status": "red", "reason": "Marketplace benchmark plans in this state exclude weight loss agents." }
      },
      {
        "if": { "medication": ["Wegovy", "Zepbound"], "bmi_lt": 27 },
        "then": { "status": "red", "reason": "BMI < 27 fails FDA clinical indication minimums." }
      },
      {
        "if": {
          "medication": ["Wegovy", "Zepbound"],
          "bmi_range": [27, 30],
          "missing_comorbidities": ["hypertension", "high_cholesterol", "diabetes", "osa", "established_cvd"]
        },
        "then": { "status": "red", "reason": "BMI 27-30 requires a qualifying comorbidity." }
      },
      {
        "if": { "medication": ["Zepbound"], "has_comorbidity": "osa" },
        "then": { "status": "yellow", "reason": "High Probability: Zepbound preferred for Obstructive Sleep Apnea." }
      },
      {
        "if": { "medication": ["Wegovy"], "has_comorbidity": "established_cvd" },
        "then": { "status": "yellow", "reason": "High Probability: FDA approved for cardiovascular risk reduction." }
      },
      {
        "if": {
           "medication": ["Wegovy", "Zepbound"],
           "missing_history": ["phentermine", "qsymia", "contrave"]
        },
        "then": { "status": "yellow", "reason": "Step Therapy: Must fail a preferred oral agent (e.g., Phentermine) first." }
      },
      {
        "if": { "program_enrollment_required": true },
        "then": { "status": "yellow", "reason": "Requires enrollment in 'Total Weight Support' (Real Appeal / WW) for authorization." }
      }
    ],
    "pricing": {
      "green_copay": 25,
      "lifestyle_program_discount": true
    }
  },
  "Medicare": {
    "id": "medicare_2026",
    "default_status": "red",
    "member_id_validation": {
      "regex": "^[1-9][A-HJ-NPR-Z][0-9A-HJ-NPR-Z][0-9][A-HJ-NPR-Z][0-9A-HJ-NPR-Z][0-9][A-HJ-NPR-Z]{2}\\d{2}$",
      "min_length": 11,
      "max_length": 11,
      "help_text": "Must be exactly 11 chars. No S, L, O, I, B, Z."
    },
    "rules": [
      {
        "if": { "medication": ["Compounded"] },
        "then": { "status": "red", "reason": "Statutory Exclusion: Non-FDA approved bulk products not covered." }
      },
      {
        "if": {
          "medication": ["Ozempic", "Mounjaro", "Trulicity"],
          "missing_comorbidities": ["diabetes"]
        },
        "then": { "status": "red", "reason": "Strict Exclusion: Diagnosis mismatch. Covered for T2D only." }
      },
      {
        "if": {
          "medication": ["Wegovy"],
          "has_comorbidity": "established_cvd",
          "bmi_ge": 27
        },
        "then": { "status": "green", "reason": "Covered for CVD Risk Reduction (The 'CVD Loophole')." }
      },
      {
        "if": {
          "medication": ["Wegovy", "Zepbound"],
          "date_lt": "2026-07-01"
        },
        "then": { "status": "red", "reason": "Statutory Exclusion until CMS 'BALANCE' Pilot begins July 1, 2026." }
      },
      {
        "if": {
          "medication": ["Wegovy", "Zepbound"],
          "date_ge": "2026-07-01",
          "bmi_ge": 35
        },
        "then": { "status": "yellow", "reason": "Conditional Coverage: CMS Pilot Program (Requires Enrollment)." }
      }
    ],
    "pricing": {
      "green_copay": 50,
      "lifestyle_program_discount": true
    }
  },
  "Medicaid": {
    "id": "medicaid_2026",
    "default_status": "red",
    "member_id_validation": {
      "regex": "^[A-Z0-9]{8,12}$",
      "min_length": 8,
      "max_length": 12,
      "help_text": "Varies by state (e.g. 9 digits for CA/TX, 10 for PA)."
    },
    "rules": [
      {
        "if": { "has_comorbidity": "diabetes" },
        "then": { "status": "green", "reason": "Covered benefit for Type 2 Diabetes." }
      },
      {
        "if": { "state_in": ["CA", "PA", "NC"] },
        "then": { "status": "red", "reason": "Coverage removed effective 2026 (or earlier) due to state budget cuts." }
      },
      {
        "if": { "state_not_in": ["CT", "DE", "MI", "MN", "NH", "RI", "SC", "VA", "WI"] },
        "then": { "status": "red", "reason": "Excluded Benefit: State has not opted into coverage." }
      },
      {
        "if": {
          "date_ge": "2026-05-01",
          "state_in": ["MI", "RI", "VA"] 
        },
        "then": { "status": "yellow", "reason": "Potential eligibility via CMS BALANCE Model (Federal override)." }
      },
      {
        "if": {
           "medication": ["Wegovy", "Zepbound"],
           "missing_history": ["phentermine", "orlistat", "contrave"]
        },
        "then": { "status": "red", "reason": "Strict Step Therapy: Must fail cheaper oral generics first." }
      }
    ],
    "pricing": {
      "green_copay": 0,
      "lifestyle_program_discount": false
    }
  }
}
}
